{
  "name": "bat-extension",
  "description": "manifest.json description",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:chrome": "wxt -b chrome",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare",
    "tailwindcss": "npx tailwindcss -i ./assets/tailwind.css -o ./assets/tailwind-prod.css"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@wxt-dev/module-react": "^1.1.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.5.4",
    "wxt": "^0.19.1"
  }
}
